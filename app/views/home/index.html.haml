%main#top.main
  = render '/shared/home_navbar'
  %section#home.py-0
    .bg-holder.d-none.d-md-block{:style => "background-image: url(#{image_path('home/illustrations/hero-section.png')}); background-position:right bottom;background-size:contain;"}
    / /.bg-holder

    .bg-holder.d-block.d-md-none{:style => "background-image:url(assets/img/illustrations/hero-bg.png);background-position:right top;background-size:contain;"}
    / /.bg-holder
    .container
      .row.align-items-center.min-vh-md-75
        .col-md-7.col-lg-6.py-6.text-sm-start.text-center
          %h1.mt-6.mb-sm-4.display-4.fw-semi-bold.lh-sm.fs-4.fs-lg-6.fs-xxl-7
            = "#{greeting_time},"
            = succeed "Welcome to Ams" do
              %br.d-block.d-lg-none.d-xl-block/
          %p.mb-4.fs-1 The best academic management platform
          .pt-3
            %form
              .input-group.w-xl-75.w-xxl-50.d-flex.flex-end-center
                %input#formGroupExampleInput.form-control.rounded-pill.border-0.font-base{:placeholder => "Search books, articles, keywords", :type => "text"}/
                = image_tag 'home/illustrations/search.png', alt: '', class: 'input-box-icon me-3', width: '18'
  / ============================================
  / <section> begin ============================
  %section
    .container
      .row.justify-content-center
        .col-auto.mb-5.mb-md-7
          %h1.fw-semi-bold.text-warning
            Our
            %span.text-1100 resources
      .row
        .col-sm-6.col-lg-3.mb-4.mb-lg-0.text-center
          .px-0.px-lg-3
            = image_tag 'home/gallery/researchers.png', alt: '', class: 'img-fluid mb-4', width: '100'
            %h3.h5.mb-4.font-base Researchers
            %p.lh-lg Register online Discover tools and manage alerts Learn about how to access
        .col-sm-6.col-lg-3.mb-4.mb-lg-0.text-center
          .px-0.px-lg-3
            = image_tag 'home/gallery/librarian.png', alt: '', class: 'img-fluid mb-4', width: '100'
            %h3.h5.mb-4.font-base Librarian
            %p.lh-lg Manage your account View products and solutions Find resources and support
        .col-sm-6.col-lg-3.mb-4.mb-lg-0.text-center
          .px-0.px-lg-3
            = image_tag 'home/gallery/societies.png', alt: '', class: 'img-fluid mb-4', width: '100'
            %h3.h5.mb-4.font-base Societies
            %p.lh-lg Publish with Wiley Explore our resource library Learn about topics and trends
        .col-sm-6.col-lg-3.mb-4.mb-lg-0.text-center
          .px-0.px-lg-3
            = image_tag 'home/gallery/authors.png', alt: '', class: 'img-fluid mb-4', width: '100'
            %h3.h5.mb-4.font-base Authors
            %p.lh-lg Submit a paper Track your article Learn about Open Access
    / end of .container
  / <section> close ============================
  / ============================================
  %section#libraries
    .bg-holder{:style => "background-image: url(#{image_path('home/illustrations/libraries-bg.png')}); background-position:left bottom;background-size:contain;"}
    / /.bg-holder
    .container
