!!!
%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %title AmsApp
    = csrf_meta_tags
    = csp_meta_tag
    = stylesheet_link_tag 'student', media: 'all', 'data-turbolinks-track': 'reload'
    = javascript_pack_tag 'student', 'data-turbolinks-track': 'reload'
    = favicon_link_tag asset_path('ams_app_icon.png')
    %script{:src => "https://kit.fontawesome.com/6d90f39943.js"}
  %body.hold-transition.sidebar-mini.layout-fixed
    .wrapper
      = render "layouts/student_nav"
      = render "layouts/student_side_nav"
      .content-wrapper
        = yield
      = render "layouts/student_footer"
